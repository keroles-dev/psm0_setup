super + Return
	st zsh

super + shift + w
	firefox-developer-edition

F2
	xbacklight -dec 5

F3
	xbacklight -inc 5

F6
	amixer set Master toggle

F7
	amixer -D pulse sset Master 1%-

F8
	amixer -D pulse sset Master 1%+

F9
	amixer set Capture toggle

F10
	amixer -D pulse sset Capture 5%-

F11
	amixer -D pulse sset Capture 5%+

super + equal
	sh ~/src/script/mount.sh

super + shift + equal
	jmtpfs -o ~/mnt

super + minus
	fusermount -u ~/mnt

super + BackSpace
	sh ~/src/script/exit.sh

Print
	maim -u -i $(xdotool getactivewindow) | xclip -selection clipboard -t image/png

ctrl + Print
	maim -s | xclip -selection clipboard -t image/png

ctrl + shift + Print
	 maim -s ~/Pictures/screenshots/$(date +%s).png

